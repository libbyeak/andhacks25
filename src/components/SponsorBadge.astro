---
import Image from "astro/components/Image.astro";

export interface Props {
    name: string;
    tier: string;
    src: string;
    alt: string;
    width: number;
    height: number
}
const {name, tier, src, alt, width, height} = Astro.props;
---
<div class="flex flex-col p-2 m-2 border-1 border-black rounded-md justify-center items-center">
<img
    width={width}
    height={height}
    src={src}
    alt={alt}
/>
</div>